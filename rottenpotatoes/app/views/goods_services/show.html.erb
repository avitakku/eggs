<h2>Details about <em><%= @good_service.name %></em> offered by <em><%= @good_service.user.name %></em></h2>

<ul id="Contact the Owner">
  <li>
    Name:
    <%= link_to "#{@good_service.user.name}", profile_path(@good_service.user)%>
  </li>
  <li>
    Email:
    <%= @good_service.user.contact_information %>
  </li>
</ul>

<%= link_to 'Go Back', goods_services_path, class: 'btn btn-primary' %>
