<!-- app/views/sessions/new.html.erb -->
<h2>Log in</h2>

<%= form_for :session, url: session_path do |f| %>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email %>
  </div>

  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </div>

  <div class="actions">
    <%= link_to 'Log In', goods_services_path, class: 'btn btn-primary' %>
  </div>
<% end %>
